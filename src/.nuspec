<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
    <metadata>
        <!-- Required elements-->
        <id>SpreadSheetParser</id>
        <version>0.0.2</version>
        <description>SpreadSheetParser is a library to parse a spreadsheet file and return a list of objects.</description>
        <authors>Alan Barros</authors>
        <repository type="git" url="https://github.com/alanbarros/SpreadSheetParser" branch="main" />
        <!-- Optional elements -->
        <!-- ... -->

        <license type="expression">MIT</license>
        <dependencies>
            <group targetFramework="netstandard2.1">
                <dependency id="ExcelDataReader" version="3.6.0" />
                <dependency id="System.Text.Encoding.CodePages" version="6.0.0" />
            </group>
        </dependencies>
    </metadata>

    <files>
        <file src="..\LICENSE.txt" target="" />
        <file src="..\README.md" target="" />
        <file src="bin\Debug\netstandard2.1\*.pdb" target="lib\netstandard2.1\" />
        <file src="bin\Debug\netstandard2.1\*.dll" target="lib\netstandard2.1\" />
    </files>
</package>